<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}SnoozIt | {% endblock %}</title>
    {#<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>#}
    {% block stylesheets %}
        {% stylesheets filter='cssrewrite'
        'bundles/snoozitplatform/css/bootstrap.min.css'
        'bundles/snoozitplatform/css/jquery-ui.min.css'
        'bundles/snoozitplatform/css/style.css'
        'bundles/snoozitplatform/css/font-awesome.min.css'
        'bundles/snoozitplatform/css/annonce.css'
        'bundles/snoozitplatform/css/dashboard.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

<div class="container">
    <div class="row">
        {% block header %}
            {{ render(controller('SnoozitPlatformBundle:SearchEngine:getHeaderTemplate')) }}
        {% endblock %}

        {% block body %}{% endblock %}
    </div>
</div>


{% block javascripts %}
    {% javascripts
    'bundles/snoozitplatform/js/jquery-2.1.3.min.js'
    'bundles/snoozitplatform/js/jquery-ui.min.js'
    'bundles/snoozitplatform/js/bootstrap.min.js'
    'bundles/snoozitplatform/js/platform.js'
    'bundles/snoozitplatform/js/abonnement.js'
    'bundles/snoozitplatform/js/autocomplete.js'
    'bundles/snoozitplatform/js/mustache.js'
    'bundles/snoozitplatform/js/jquery.notif.js'
    %}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script type="text/javascript" src="{{ asset_url }}"></script>

    {% endjavascripts %}
{% endblock %}
</body>
</html>
