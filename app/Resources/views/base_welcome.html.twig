<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}SnoozIt |{% endblock %}</title>
    {% block stylesheets %}
        {% stylesheets filter='cssrewrite'
            'bundles/snoozitplatform/css/bootstrap.min.css'
            'bundles/snoozitplatform/css/welcome.css'
            'bundles/snoozitplatform/css/font-awesome.min.css'
        %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

<div class="container-fluid">
    <div class="row">
        {% block body %}{% endblock %}
    </div>
</div>

{% block javascripts %}
    {% javascripts
        'bundles/snoozitplatform/js/jquery-2.1.3.min.js'
        'bundles/snoozitplatform/js/bootstrap.min.js'
        'bundles/snoozitplatform/js/raphael.min.js'
        'bundles/snoozitplatform/js/mousewheel.min.js'
        'bundles/snoozitplatform/js/mapael/jquery.mapael.js'
        'bundles/snoozitplatform/js/mapael/maps/france_departments.js'
        'bundles/snoozitplatform/js/sz_mapel.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>

    {% endjavascripts %}
{% endblock %}

</body>
</html>
